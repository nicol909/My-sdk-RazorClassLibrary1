    <AddRazorSupportForMvc>true</AddRazorSupportForMvc>
    <RootNamespace>My_sdk_RazorClassLibrary1</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.Razor" Version="3.1.0" />
    <PackageReference Include="Microsoft.NET.Sdk.Razor" Version="3.1.0" />
    <PackageReference Include="RazorGenerator.MsBuild" Version="2.5.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="ServiceStack.Razor" Version="5.7.0" />
    <PackageReference Include="Westwind.RazorHosting" Version="3.3.9" />
  </ItemGroup>


</Project>
